<Window x:Class="Gibjohn_tutoring.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gibjohn_tutoring"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Center" Margin="0,199,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="138" Name="UsernameTextBox"/>
        <PasswordBox HorizontalAlignment="Center" Margin="0,241,0,0" VerticalAlignment="Top" Width="138" Name="PasswordBox"/>
        <TextBlock HorizontalAlignment="Left" Height="18" Margin="251,199,0,0" TextWrapping="Wrap" Text="Username:" VerticalAlignment="Top" Width="97"/>
        <TextBlock HorizontalAlignment="Left" Height="13" Margin="251,243,0,0" TextWrapping="Wrap" Text="Password:" VerticalAlignment="Top" Width="97"/>
        <Button Name="LoginButton" Content="Login" HorizontalAlignment="Center" Height="31" Margin="0,287,0,0" VerticalAlignment="Top" Width="138" Click="LoginButton_Click_1"/>
        <TextBlock HorizontalAlignment="Left" Height="30" Margin="290,144,0,0" TextWrapping="Wrap" FontWeight="Heavy" Text="GibJohn Tutoring System" VerticalAlignment="Top" Width="179"/>

    </Grid>
</Window>
